uter-link to=\"/admin/projects\" class=\"nav-item\">\r\n          <i class=\"fas fa-project-diagram\"></i>\r\n          <span>Проекты</span>\r\n        </router-link>\r\n        <router-link to=\"/admin/services\" class=\"nav-item\">\r\n          <i class=\"fas fa-tools\"></i>\r\n          <span>Услуги</span>\r\n        </router-link>\r\n        <router-link to=\"/admin/applications\" class=\"nav-item\">\r\n          <i class=\"fas fa-envelope\"></i>\r\n          <span>Заявки</span>\r\n        </router-link>\r\n        <button @click=\"logout\" class=\"nav-item logout\">\r\n          <i class=\"fas fa-sign-out-alt\"></i>\r\n          <span>Выйти</span>\r\n        </button>\r\n      </nav>\r\n    </aside>\r\n    <main class=\"admin-main\">\r\n      <header class=\"admin-header\">\r\n        <div class=\"header-content\">\r\n          <h1>{{ pageTitle }}</h1>\r\n          <div class=\"admin-user\">\r\n            <span>{{ adminName }}</span>\r\n            <img :src=\"adminAvatar\" alt=\"Admin\" class=\"avatar\">\r\n          </div>\r\n        </div>\r\n      </header>\r\n      <div class=\"admin-content\">\r\n        <router-view></router-view>\r\n      </div>\r\n    </main>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, computed } from 'vue'\r\nimport { useRouter, useRoute } from 'vue-router'\r\n\r\nconst router = useRouter()\r\nconst route = useRoute()\r\n\r\nconst adminName = ref('Администратор')\r\nconst adminAvatar = ref('https://via.placeholder.com/40')\r\n\r\nconst pageTitle = computed(() => {\r\n  const titles = {\r\n    '/admin/dashboard': 'Дашборд',\r\n    '/admin/projects': 'Управление проектами',\r\n    '/admin/services': 'Управление услугами',\r\n    '/admin/applications': 'Заявки'\r\n  }\r\n  return titles[route.path] || 'Панель управления'\r\n})\r\n\r\nconst logout = () => {\r\n  // Implement logout logic here\r\n  router.push('/admin/login')\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '@/assets/styles/main.scss'