       {{ feature }}\r\n                </li>\r\n              </ul>\r\n              <router-link to=\"/contacts\" class=\"btn btn-primary\">Get a Quote</router-link>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Process Section -->\r\n    <section class=\"process\">\r\n      <div class=\"container\">\r\n        <h2 class=\"section-title text-center\">Our Process</h2>\r\n        <div class=\"process-steps\">\r\n          <div class=\"process-step\" v-for=\"(step, index) in processSteps\" :key=\"index\">\r\n            <div class=\"step-number\">{{ index + 1 }}</div>\r\n            <h3>{{ step.title }}</h3>\r\n            <p>{{ step.description }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, computed } from 'vue'\r\n\r\nconst categories = ref([\r\n  { id: 'all', name: 'All Services' },\r\n  { id: 'design', name: 'Design' },\r\n  { id: 'construction', name: 'Construction' },\r\n  { id: 'renovation', name: 'Renovation' }\r\n])\r\n\r\nconst selectedCategory = ref('all')\r\n\r\nconst services = ref([\r\n  {\r\n    id: 'architectural-design',\r\n    category: 'design',\r\n    title: 'Architectural Design',\r\n    description: 'Comprehensive architectural solutions for residential and commercial projects.',\r\n    image: '/images/services/architectural-design.jpg',\r\n    features: [\r\n      'Custom architectural plans',\r\n      '3D visualization',\r\n      'Building code compliance',\r\n      'Sustainable design options'\r\n    ]\r\n  },\r\n  {\r\n    id: 'interior-design',\r\n    category: 'design',\r\n    title: 'Interior Design',\r\n    description: 'Creative interior solutions that reflect your style and meet your needs.',\r\n    image: '/images/services/interior-design.jpg',\r\n    features: [\r\n      'Space planning',\r\n      'Material selection',\r\n      'Lighting design',\r\n      'Furniture layout'\